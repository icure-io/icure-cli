{
	"name": "icure-cli",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"build": "yarn run lint && tsc",
		"start": "yarn run build && node --experimental-specifier-resolution=node dist/icure-cli.js",
		"lint": "eslint . --fix --ext .ts --ignore-pattern \"**/*.d.ts\"",
		"test": "jest",
		"prepare": "rimraf dist && yarn run build",
		"precommit": "pretty-quick --staged",
		"package": "yarn run build && mkdirp dist/icure-cli/ && nexe dist/icure-cli.js --output dist/icure-cli/icure-cli"
	},
	"bin": "dist/icure-cli.js",
	"type": "module",
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/isomorphic-fetch": "^0.0.36",
		"@types/jest": "^27.4.1",
		"@types/lodash": "^4.14.181",
		"@types/node": "^17.0.23",
		"@types/node-localstorage": "^1.3.0",
		"@types/vorpal": "^1.12.0",
		"@typescript-eslint/eslint-plugin": "^5.18.0",
		"@typescript-eslint/eslint-plugin-tslint": "^5.18.0",
		"@typescript-eslint/parser": "^5.18.0",
		"eslint": "^7.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsdoc": "^39.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.5.1",
		"mixin-deep": "^2.0.1",
		"prettier": "^2.6.2",
		"pretty-quick": "^3.1.3",
		"rimraf": "^3.0.2",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"@icure/api": "^4.0.8",
		"@icure/medical-device-sdk": "^0.1.18",
		"colors": "^1.4.0",
		"date-fns": "^2.3.0",
		"isomorphic-fetch": "^3.0.0",
		"lodash": "^4.17.20",
		"mkdirp": "^1.0.4",
		"nexe": "^3.3.7",
		"node-localstorage": "^2.2.1",
		"pegjs": "^0.10.0",
		"pkg": "^5.6.0",
		"vorpal": "^1.12.0",
		"xlsx": "^0.18.5",
		"xml2js": "^0.4.22"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.jsx": [
			"eslint --fix",
			"git add"
		],
		"*.ts": [
			"eslint --fix",
			"git add"
		],
		"*.tsx": [
			"eslint --fix",
			"git add"
		]
	},
	"prettier": {
		"semi": false,
		"printWidth": 100,
		"singleQuote": true
	},
	"packageManager": "yarn@3.2.0"
}
